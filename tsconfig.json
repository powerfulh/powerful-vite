{
	"compilerOptions": {
		"types": [
			"vite/client"
		],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"moduleResolution": "Node",
		"module": "es2022",
		"lib": [
			"es2022",
			"Dom"
		], // 2018: finally, 2021: replaceAll, 2022: Array.prototype.at
		"baseUrl": "./",
		"paths": {
			"@/*": [
				"src/*"
			]
		},
		"allowJs": true,
		"outDir": "out",
		"noImplicitThis": true
	},
	// "exclude": ["dist"]
}